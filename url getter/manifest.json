{
    "manifest_version": 2,
    "name": "URL Getter",
    "description": "This extension gets the URL of the current tab",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"], 
            "js": ["script.js"]
        }
    ]
}